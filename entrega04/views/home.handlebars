<div id="main">
    <div id="realTimeProducts">
        <h2>Listado de productos (desde home.handlebars):</h2>
        {{#each products}}
            <div id ="divProductos">
                <p>{{this.title}}</p>
                <p>{{this.description}}</p>
                <p>{{this.category}}</p>
                <p>${{this.price}}</p>
                <p>{{this.stock}}</p>
            </div>
        {{/each}}
    </div>
</div>
